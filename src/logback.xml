<configuration scan="false">
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>messagebackup.log</file>
		<append>true</append>
		<encoder>
			<pattern>%7relative %-5level  %logger | %msg%n</pattern>
		</encoder>
	</appender>
	<root level="trace">
		<appender-ref ref="FILE" />
	</root>
</configuration>
